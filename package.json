{
  "name": "react-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "json-server": "json-server --watch -p 8080 ./src/assets/db.json",
    "dev-json": "concurrently --kill-others \"yarn json-server\" \"yarn dev --debug\"",
    "build": "vite build",
    "preview": "vite preview",
