    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.3.0",
    "jsdom": "^20.0.0",
    "msw": "^0.44.2",
    "sass": "^1.54.0",
    "vite": "^3.0.0",
    "vitest": "^0.18.1",
    "whatwg-fetch": "^3.6.2"
  }
